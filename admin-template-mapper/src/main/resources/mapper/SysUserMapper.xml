<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.warmstone.admin.template.mapper.SysUserMapper">

    <select id="selectById" resultType="com.warmstone.admin.template.domain.entity.SysUser">
